{% extends"::base.html.twig" %}
{% block Page %}
    <!-- page -->
    <div class="services-breadcrumb">
        <div class="agile_inner_breadcrumb">
            <div class="container">
                <ul class="w3_short">
                    <li>
                        <a href="{{ path('souk_medina_homepage') }}">Home</a>
                        <i>|</i>
                    </li>
                    <li>Reclamations</li>
                </ul>
            </div>
        </div>
    </div>
    <!-- //page -->
    <!-- contact page -->
    <div class="contact-w3l">
        <div class="container">
            <!-- tittle heading -->
            <h3 class="tittle-w3l">Reclamations
                <span class="heading-style">
					<i></i>
					<i></i>
