                                                <option value="ROLE_GERANT">Gerant</option>
                                                <option value="ROLE_SUPER_ADMIN">Admin</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div>
                                        <input type="submit" value="{{ 'submit'|trans }}" id="_submit" name="_submit" />
                                    </div>

