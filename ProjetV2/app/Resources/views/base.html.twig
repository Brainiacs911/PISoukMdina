                                    </div>
                                    <div class="styled-input agile-styled-input-top">
                                        <input type="text" id="fos_user_registration_form_username" class="form-username form-control" name="fos_user_registration_form[username]" required="required" maxlength="180" pattern=".{2,}" placeholder="Nom d'utilisateur...">
                                    </div>

                                    <div class="styled-input">
                                        <input type="password" id="fos_user_registration_form_plainPassword_first" class="form-username form-control" name="fos_user_registration_form[plainPassword][first]" required="required" autocomplete="new-password" placeholder="Mot de passe...">
                                    </div>
                                    <div class="styled-input">
                                        <input type="password" id="fos_user_registration_form_plainPassword_second" class="form-username form-control" name="fos_user_registration_form[plainPassword][second]" required="required" autocomplete="new-password" placeholder="Confirmation de mot de passe...">
                                    </div>
                                    <div class="form-group">
                                        <div class="form-group">
                                            <label for="fos_user_registration_form_roles" class="sr-only">Role</label>
                                            <select id="fos_user_registration_form_roles" name="fos_user_registration_form[roles][]" required="required" class="form-username form-control">
